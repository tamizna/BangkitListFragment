<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tamizna.bangkitlistfragment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BangkitListFragment">
        <activity android:name=".learnViewBinding.AnimalActivity"></activity>
        <activity android:name=".learnFragment.ShowFragmentActivity" />
        <activity android:name=".learnList.recyclerView.DetailFruitActivity" />
        <activity android:name=".learnList.recyclerView.RvActivity" />
        <activity android:name=".learnList.listView.ListViewActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>